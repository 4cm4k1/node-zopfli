{
  "author": "Pierre Inglebert <pierre.inglebert@gmail.com>",
  "name": "node-zopfli",
  "description": "Bindings for Zopfli compressing lib. Compress gzip files 5% better than gzip.",
  "contributors": [
    "duralog <funisher@gmail.com>"
  ],
  "keywords": [
    "zopfli",
    "zlib",
    "compress",
    "gzip",
    "deflate"
  ],
  "version": "1.2.6",
  "homepage": "https://github.com/pierreinglebert/node-zopfli",
  "repository": {
    "type": "git",
    "url": "https://github.com/pierreinglebert/node-zopfli.git"
  },
  "engines": {
    "node": ">=0.10"
  },
  "binary": {
    "module_name": "zopfli",
    "module_path": "./lib/binding/{node_abi}-{platform}-{arch}",
    "remote_path": "./{configuration}",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
    "host": "https://node-zopfli.s3.amazonaws.com"
  },
  "dependencies": {
    "bluebird": "~2.9.1",
    "buffer-crc32": "~0.2.1",
    "commander": "~2.7.0",
    "defaults": "^1.0.2",
    "nan": "~1.6.0",
    "node-pre-gyp": "~0.6.4"
  },
  "devDependencies": {
    "async": "~0.9.0",
    "blanket": "~1.1.5",
    "chai": "~2.1.0",
    "coveralls": "~2.11.0",
    "jshint": "~2.6.0",
    "mocha": "~1.19.0",
    "mocha-lcov-reporter": "~0.0.1",
    "randomstring": "^1.0.3"
  },
  "bin": [
    "bin/zopfli",
    "bin/zopflipng"
  ],
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build --build-from-source",
    "test": "make test"
  },
  "config": {
    "blanket": {
      "pattern": "lib",
      "data-cover-never": "node_modules"
    }
  },
  "main": "lib/zopfli.js",
  "gypfile": true
}
